{{>layouts/header}}
<table class="table p-5">
    <thead>
    <tr>
        <th scope="col">ID</th>
        <th scope="col">Title</th>
        <th scope="col">Content</th>
    </tr>
    </thead>
    <tbody>
    {{#articleList}}
    <tr>
        <td>{{id}}</td>
        <td><a href="/articles/{{id}}">{{title}}</a></td>
        <td>{{content}}</td>
    </tr>
    </tbody>
    {{/articleList}}
</table>

<a href="/articles/new" class="btn btn-primary">새글쓰기</a>
{{>layouts/footer}}